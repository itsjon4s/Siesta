{
  "name": "siesta",
  "private": true,
  "version": "0.0.1",
  "description": "Siesta is a Discord bot used in +200 servers. Built with Node.js, TypeScript and discord.js.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/itsjon4s/SiestaRewrite.git"
  },
  "bugs": {
    "url": "https://github.com/itsjon4s/SiestaRewrite/issues"
  },
  "homepage": "https://github.com/itsjon4s/SiestaRewrite#readme",
  "author": "Jon4s",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=16.9.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "node dist/src/index.js",
    "dev": "tsnd --expose-gc --transpile-only --ignore-watch node_modules --respawn src/index.ts",
    "types:check": "tsc --noEmit",
    "lint:fix": "eslint src --fix",
    "lint": "eslint"
  },
  "dependencies": {
    "@prisma/client": "4.2.1",
    "chalk": "^4.0.0",
    "discord.js": "^14.1.2",
    "glob": "^7.2.0",
    "undici": "^5.8.2",
    "vulkava": "^1.5.5",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "eslint": "8.21.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.7.1",
    "prisma": "4.2.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
