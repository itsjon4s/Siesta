generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type GuildsAntiinvite {
  status    Boolean
  whitelist Json?
}

type GuildsWelcome {
  channel String?
  message String?
  status  Boolean
}


model guilds {
  id         String           @id @map("_id")
  v          Int              @map("__v")
  antiinvite GuildsAntiinvite
  prefix     String 
  welcome    GuildsWelcome
}

model users {
  id              String          @id @map("_id")
  v               Int             @map("__v")
  blacklist       Boolean
}
